AnnotationAssertion(rdfs:comment ease:AgeObservation "Age of a person calculated from the birth date."^^xsd:string)
EquivalentClasses(ease:AgeObservation ObjectIntersectionOf(ObjectSomeValuesFrom(<http://org.hl7.v3/rim/upper#code> ObjectHasValue(<http://org.hl7.v3/rim/terms#denotes> ease:person_age)) ease:Observation))
SubClassOf(ease:BiologicalProcess ease:Process)
AnnotationAssertion(rdfs:comment ease:CodedConcept "Grouping class for codes from internal or external terminology systems such as MCBT or SNOMED CT, LOINC etc.")
SubClassOf(ease:CodedConcept ease:InformationContentEntity)
SubClassOf(ease:ConceptFromLOINC ease:CodedConcept)
SubClassOf(ease:ConceptFromMayoDataStandard ease:CodedConcept)
SubClassOf(ease:ConceptFromSNOMED ease:CodedConcept)
AnnotationAssertion(rdfs:comment ease:EASEThing "The root for all Mayo EASE classes")
SubClassOf(ease:Hospitalization ease:PatientEncounter)
AnnotationAssertion(rdfs:label ease:InformationContentEntity "Information content entity")
AnnotationAssertion(rdfs:comment ease:InformationContentEntity "an information content entity is an entity that is generically dependent on some material entity or process and stands in relation of aboutness to some entity or process.")
AnnotationAssertion(rdfs:seeAlso ease:InformationContentEntity "http://purl.obolibrary.org/obo/IAO_0000030")
SubClassOf(ease:InformationContentEntity ease:EASEThing)
SubClassOf(ease:MaterialEntity ease:EASEThing)
AnnotationAssertion(rdfs:comment ease:Observation "Modeled on RIM's core:Observation. Therefore, as with all Act statements, Observation statements describe what was done, and in the case of Observations, this includes a description of what was actually observed (results or answers); and those results or answers are part of the observation and not split off into other objects.")
AnnotationAssertion(rdfs:comment ease:Observation "An 'Observation' is the output of an observation process. It can be of  continuous, categorical or a boolean type.
     Eg.: counts, length, weight, concentration, rate, etc.")
EquivalentClasses(ease:Observation ObjectIntersectionOf(<http://org.hl7.v3/rim/upper#Event> <http://org.hl7.v3/rim/core#Observation>))
SubClassOf(ease:Observation ease:InformationContentEntity)
SubClassOf(ease:PatientEncounter ease:PlannedProcess)
AnnotationAssertion(rdfs:comment ease:PatientEntity "a person who is in a role of a patient.")
EquivalentClasses(ease:PatientEntity ObjectIntersectionOf(ObjectSomeValuesFrom(<http://org.hl7.v3/rim/upper#player> ease:PatientRole) ease:Person))
SubClassOf(ease:PatientRole ease:Role)
AnnotationAssertion(rdfs:label ease:Person "Person"^^xsd:string)
AnnotationAssertion(<http://www.w3.org/2004/02/skos/core#altLabel> ease:Person "Human"^^xsd:string)
SubClassOf(ease:Person ease:MaterialEntity)
SubClassOf(ease:Person ObjectSomeValuesFrom(<http://org.hl7.v3/rim/upper#name> ObjectHasValue(<http://org.hl7.v3/rim/terms#denotes> ease:person_name)))
SubClassOf(ease:Person ObjectSomeValuesFrom(ease:hasAge ease:AgeObservation))
SubClassOf(ease:Person ObjectSomeValuesFrom(ease:hasPicture ObjectHasValue(<http://org.hl7.v3/rim/terms#denotes> ease:picture)))
SubClassOf(ease:Person ObjectSomeValuesFrom(ease:hasWeight ease:WeightObservation))
SubClassOf(ease:PlannedProcess ease:Process)
SubClassOf(ease:Process ease:EASEThing)
SubClassOf(ease:Role ease:EASEThing)
AnnotationAssertion(rdfs:comment ease:WeightAtAdmission "Earliest value for weight in kg entered after admission to the hospital."^^xsd:string)
SubClassOf(ease:WeightAtAdmission ease:WeightObservation)
SubClassOf(ease:WeightAtAdmission ObjectSomeValuesFrom(ease:outputOf ObjectSomeValuesFrom(<http://org.hl7.v3/rim/types/rel#occursDuring> ease:Hospitalization)))
AnnotationAssertion(rdfs:comment ease:WeightChangeObservation "Most recent value of “WT CHNG ADMIT (KG)” within last 24 hours, that is after admission weight"^^xsd:string)
SubClassOf(ease:WeightChangeObservation ease:WeightObservation)
SubClassOf(ease:WeightChangeObservation ObjectSomeValuesFrom(<http://org.hl7.v3/rim/upper#code> ObjectHasValue(<http://org.hl7.v3/rim/terms#denotes> ease:body_weight_change)))
AnnotationAssertion(rdfs:comment ease:WeightObservation "observation of a person's weight"^^xsd:string)
EquivalentClasses(ease:WeightObservation ObjectIntersectionOf(ease:Observation ObjectSomeValuesFrom(<http://org.hl7.v3/rim/upper#code> ObjectHasValue(<http://org.hl7.v3/rim/terms#denotes> ease:body_weight)) ObjectSomeValuesFrom(ease:isAbout ease:Person)))
AnnotationAssertion(rdfs:comment ease:WeightObservation-LatestDuringHospitalization "Most recent weight within last 24 hours that is after admission weight.")
AnnotationAssertion(rdfs:label ease:WeightObservation-LatestDuringHospitalization "Most recent weight after admission")
SubClassOf(ease:WeightObservation-LatestDuringHospitalization ease:WeightObservation)
SubClassOf(ease:WeightObservation-LatestDuringHospitalization ObjectSomeValuesFrom(ease:outputOf ObjectSomeValuesFrom(<http://org.hl7.v3/rim/types/rel#occursDuring> ease:Hospitalization)))
ObjectPropertyDomain(ease:hasAge ease:Person)
ObjectPropertyRange(ease:hasAge ease:AgeObservation)
InverseObjectProperties(ease:outputOf ease:hasOutput)
ObjectPropertyDomain(ease:hasWeight ease:Person)
ObjectPropertyRange(ease:hasWeight ease:WeightObservation)
ObjectPropertyDomain(ease:isAbout ease:InformationContentEntity)
