###  http://org.drools.conyard.owl#Activity

:Activity rdf:type owl:Class ;
          
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :hasComment ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ,
                          [ rdf:type owl:Class ;
                            owl:intersectionOf ( :Entity
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :involves ;
                                                   owl:onClass :Person ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :requires ;
                                                   owl:onClass :Equipment ;
                                                   owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :endsOn ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onDataRange xsd:dateTime
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :startsOn ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onDataRange xsd:dateTime
                                                 ]
                                               )
                          ] ;
          
          owl:hasKey ( :hasComment
                     ) .



