#################################################################
#
#    Annotation properties
#
#################################################################


rdfs:comment rdf:type owl:AnnotationProperty .



:abstract rdf:type owl:AnnotationProperty ;
          
          rdfs:range xsd:boolean ;
          
          rdfs:domain owl:Thing .





#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral

rdf:PlainLiteral rdf:type rdfs:Datatype .



###  http://www.w3.org/2000/01/rdf-schema#Literal

rdfs:Literal rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#boolean

xsd:boolean rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#dateTime

xsd:dateTime rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#float

xsd:float rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#int

xsd:int rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#string

xsd:string rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://org.drools.conyard.owl#endangersWith

:endangersWith rdf:type owl:ObjectProperty ;
               
               rdfs:range :Danger ;
               
               rdfs:domain :Dangerous .



###  http://org.drools.conyard.owl#hasSite

:hasSite rdf:type owl:AsymmetricProperty ,
                  owl:IrreflexiveProperty ,
                  owl:ObjectProperty ;
         
         rdfs:domain :ConstructionYard ;
         
         rdfs:range :Site ;
         
         owl:inverseOf :locatedAt .



###  http://org.drools.conyard.owl#involves

:involves rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ,
                   owl:ObjectProperty ;
          
          rdfs:domain :Activity ;
          
          rdfs:range :Person ;
          
          owl:inverseOf :participatesIn .



###  http://org.drools.conyard.owl#isDenotedBy

:isDenotedBy rdf:type owl:ObjectProperty ;
             
             rdfs:domain :CodedConcept ;
             
             rdfs:range :Expression .



###  http://org.drools.conyard.owl#locatedAt

:locatedAt rdf:type owl:ObjectProperty ;
           
           rdfs:range :ConstructionYard ;
           
           rdfs:domain :Site .



###  http://org.drools.conyard.owl#participatesIn

:participatesIn rdf:type owl:AsymmetricProperty ,
                         owl:IrreflexiveProperty ,
                         owl:ObjectProperty ;
                
                rdfs:range :Activity ;
                
                rdfs:domain :Person .



###  http://org.drools.conyard.owl#protectsAgainst

:protectsAgainst rdf:type owl:ObjectProperty ;
                 
                 rdfs:domain :DPI ;
                 
                 rdfs:range :Danger .



###  http://org.drools.conyard.owl#requires

:requires rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ,
                   owl:ObjectProperty ;
          
          rdfs:domain :Activity ;
          
          rdfs:range :Equipment .



###  http://org.drools.conyard.owl#storedIn

:storedIn rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ,
                   owl:ObjectProperty ;
          
          rdfs:domain :Equipment ;
          
